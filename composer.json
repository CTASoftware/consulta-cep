{
    "name": "ctasoftware/consulta-cep",
    "description": "Com esse pacote você poderá realizar consultas de CEP gratuitamente.",
    "keywords": ["cep", "correios", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Everton Gonçalves",
            "email": "everton@ctasoftware.com.br"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/dom-crawler": "^3.2",
        "symfony/css-selector": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "CTASoftware\\CepGratis\\": "src/",
            "CTASoftware\\CepGratis\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "suggest": {
        "CTASoftware/cnpj-gratis": "Permite consulta de CNPJ no site da receita",
        "CTASoftware/cpf-gratis": "Permite consulta de CPF no site da receita"
    },
    "minimum-stability": "dev"
}
